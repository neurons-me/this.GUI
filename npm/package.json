{
  "name": "this.gui",
  "private": false,
  "version": "1.3.51",
  "type": "module",
  "main": "./dist/this.gui.cjs",
  "module": "./dist/this.gui.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/this.gui.es.js",
      "require": "./dist/this.gui.cjs"
    },
    "./style.css": "./dist/this.gui.css",
    "./style": "./dist/this.gui.css",
    "./atoms": {
      "types": "./dist/atoms/index.d.ts",
      "import": "./dist/atoms/index.js",
      "require": "./dist/atoms/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "gui": "./dist/bin/cli.js"
  },
  "scripts": {
    "build": "vite build && npm run build:types && npm run build:cli && npm run build:copy-init",
    "build:types": "tsc -p tsconfig.build.json",
    "build:cli": "tsc --project tsconfig.cli.json && node -e \"import('./vite.config.js').then(m => (m.buildFinished ? m.buildFinished() : Promise.reject(new Error('buildFinished() not exported from vite.config.js')))).catch(e => { console.error(e); process.exit(1); })\"",
    "build:copy-init": "node -e \"const fs=require('fs-extra'); if(!fs.existsSync('init')){throw new Error('init directory missing at project root');} fs.removeSync('dist/init'); fs.copySync('init','dist/init');\"",
    "pack:local": "npm run build && npm pack",
    "prepublishOnly": "npm run build",
    "dev": "vite",
    "dev:lib": "vite build --watch",
    "preview": "vite preview",
    "storybook": "STORYBOOK=true storybook dev -p 6006",
    "build-storybook": "STORYBOOK=true storybook build",
    "chromatic": "STORYBOOK=true chromatic --project-token=$CHROMATIC_PROJECT_TOKEN"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@lexical/link": "^0.33.1",
    "@lexical/list": "^0.33.1",
    "@lexical/react": "^0.33.1",
    "@lexical/rich-text": "^0.33.1",
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "@mui/material": "^6.4.6",
    "file-saver": "^2.0.5",
    "fs-extra": "^11.3.2",
    "js-cookie": "^3.0.5",
    "js-sha3": "^0.9.3",
    "kolorist": "^1.8.0",
    "lexical": "^0.33.1",
    "material-symbols": "^0.35.2",
    "prompts": "^2.4.2",
    "qrcode-generator": "^2.0.4",
    "qrcode.react": "^4.2.0",
    "react-draggable": "^4.4.6",
    "react-feather": "^2.0.10",
    "react-syntax-highlighter": "^15.6.6",
    "react-webcam": "^7.2.0",
    "this.me": "^3.0.34",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.21.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@storybook/addon-a11y": "^9.1.4",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-links": "^9.1.17",
    "@storybook/addon-onboarding": "^9.1.4",
    "@storybook/addon-themes": "^9.1.4",
    "@storybook/addon-vitest": "^9.1.4",
    "@storybook/react-vite": "^9.1.4",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.10.4",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "chromatic": "^13.1.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.1.4",
    "globals": "^15.15.0",
    "playwright": "^1.55.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.2",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "react-router-dom": {
      "optional": true
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
