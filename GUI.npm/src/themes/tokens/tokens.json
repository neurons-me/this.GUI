{
  "$description": "Design tokens for This.GUI \u2014 synced with MUI theme.",
  "$themes": [
    {
      "id": "light",
      "name": "Light",
      "selectedTokenSets": {
        "global": "source",
        "light": "enabled"
      }
    },
    {
      "id": "dark",
      "name": "Dark",
      "selectedTokenSets": {
        "global": "source",
        "dark": "enabled"
      }
    }
  ],
  "global": {
    "radius": {
      "md": {
        "$type": "radius",
        "$value": "10px"
      }
    },
    "spacing": {
      "base": {
        "$type": "spacing",
        "$value": "8"
      }
    },
    "font": {
      "family": {
        "$type": "fontFamily",
        "$value": "Roboto, sans-serif"
      },
      "weightBold": {
        "$type": "fontWeight",
        "$value": "700"
      },
      "weightSemibold": {
        "$type": "fontWeight",
        "$value": "600"
      },
      "sizeBase": {
        "$type": "fontSize",
        "$value": "16"
      },
      "letterTight": {
        "$type": "letterSpacing",
        "$value": "-0.01em"
      }
    }
  },
  "light": {
    "color": {
      "primary": {
        "$type": "color",
        "$value": "#0a3a42"
      },
      "secondary": {
        "$type": "color",
        "$value": "#e5396f"
      },
      "icon": {
        "$type": "color",
        "$value": "#5e5e5e"
      },
      "bgDefault": {
        "$type": "color",
        "$value": "#f8f9fa"
      },
      "bgPaper": {
        "$type": "color",
        "$value": "#ffffff"
      },
      "bgNav": {
        "$type": "color",
        "$value": "#fdfdfd"
      },
      "textPrimary": {
        "$type": "color",
        "$value": "#111111"
      },
      "textSecondary": {
        "$type": "color",
        "$value": "#444444"
      },
      "link": {
        "$type": "color",
        "$value": "#008c7d"
      },
      "linkVisited": {
        "$type": "color",
        "$value": "#006e64"
      },
      "border": {
        "$type": "color",
        "$value": "rgba(0,0,0,0.08)"
      }
    }
  },
  "dark": {
    "color": {
      "primary": {
        "$type": "color",
        "$value": "#90caf9"
      },
      "secondary": {
        "$type": "color",
        "$value": "#f48fb1"
      },
      "icon": {
        "$type": "color",
        "$value": "#a8a8a8"
      },
      "bgDefault": {
        "$type": "color",
        "$value": "#121214"
      },
      "bgPaper": {
        "$type": "color",
        "$value": "#181a1c"
      },
      "bgNav": {
        "$type": "color",
        "$value": "#16181a"
      },
      "textPrimary": {
        "$type": "color",
        "$value": "#ffffff"
      },
      "textSecondary": {
        "$type": "color",
        "$value": "rgba(255,255,255,0.75)"
      },
      "link": {
        "$type": "color",
        "$value": "#00aa96"
      },
      "linkVisited": {
        "$type": "color",
        "$value": "#008278"
      },
      "border": {
        "$type": "color",
        "$value": "rgb(45,45,55)"
      }
    }
  }
}